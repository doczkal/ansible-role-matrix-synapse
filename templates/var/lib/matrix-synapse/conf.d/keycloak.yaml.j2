## keycloak ##

password_providers:
  - module: "keycloak_auth_provider.KeycloakAuthProvider"
    config:
      enabled: true
      url: "https://login.{{ synapse_server_name }}:8443/auth/"
      client_id: "synapse-jwt"
      realm_name: "{{ synapse_server_name }}"
      secret_key: "aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee"
      public_key: ""
      algorithm: "RS256"
      profile_attrs:
        display_name: displayName
        medium: medium
        address: address
